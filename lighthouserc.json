{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:8080/",
        "http://localhost:8080/blog",
        "http://localhost:8080/about"
      ]
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "unsized-images": "off",
        "uses-responsive-images": "off",
        "csp-xss": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
